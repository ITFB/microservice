<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.richard.springcloud.dao.TbItemMapper">
    <select id="findById" resultType="tbitem" parameterType="long">
      select * from tb_item where id = #{id}
  </select>

    <select id="findAll" resultType="tbitem">
      select * from tb_item
  </select>
</mapper>